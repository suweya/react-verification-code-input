{
  "name": "react-verification-code-input",
  "version": "1.2.9",
  "description": "ðŸŽ‰A verification code input",
  "author": "suweya",
  "license": "MIT",
  "repository": "https://github.com/suweya/react-verification-code-input",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 jest --testResultsProcessor ./node_modules/jest-sonar-reporter",
    "test:watch": "jest --watch --testResultsProcessor ./node_modules/jest-sonar-reporter",
    "test:coverage": "jest --coverage --watchAll=false --colors --testResultsProcessor ./node_modules/jest-sonar-reporter",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0 || ^17.0.0",
    "react-dom": "^15.0.0 || ^16.0.0 || ^17.0.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "react-component",
    "code",
    "input",
    "verify",
    "verifycode",
    "PIN",
    "pin-code"
  ],
